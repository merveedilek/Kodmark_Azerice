<div class="container" style="width:80%; margin:auto">
    <div id="about" class="section wb">

        <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
            <div class="carousel-inner sliderMobil" style="width:70%; margin:auto; border-radius:10% 10%; border:1px;">
                <div class="carousel-item active">
                    <img class="d-block w-100 sliderMobil" src="~/Content/Kodmark/slides/1s.jpg" alt="First slide" style="width:100%; height:389px">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100 sliderMobil" src="~/Content/Kodmark/slides/3s.jpg" alt="Second slide" style="width:100%; height:389px">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100 sliderMobil" src="~/Content/Kodmark/slides/6s.jpg" alt="Third slide" style="width:100%; height:389px">
                </div>
            </div>
            <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </div><!-- end container -->

